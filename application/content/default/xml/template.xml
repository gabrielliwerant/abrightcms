<?xml version="1.0" encoding="UTF-8"?>

<root>

	<title>Template</title>
	<description>Stores template content and metadata for our html document</description>
	
	<head>
		<head_doc>
			<doctype></doctype>
			<html_dir>ltr</html_dir>
			<html_lang>en-US</html_lang>
			<title>A Bright CMS</title>
		</head_doc>
		
		<title_page>
			<welcome>
				<text>Welcome</text>
				<submenu></submenu>
			</welcome>
			<error>
				<text>Error</text>
				<submenu></submenu>
			</error>
		</title_page>
		
		<head_meta>
			<name>
				<description></description>
				<keywords></keywords>
				<author></author>
				<viewport>width=device-width, maximum-scale=1.0, minimum-scale=1.0, initial-scale=1.0</viewport>
			</name>
			
			<http-equiv>
				<Content-Type>text/html; charset=utf-8</Content-Type>
			</http-equiv>
		</head_meta>
		
		<head_includes>
			<head_css>
				<style>
					<href></href>
					<is_internal>1</is_internal>
					<ie_conditional></ie_conditional>
				</style>
				<ie9>
					<href></href>
					<is_internal>1</is_internal>
					<ie_conditional>IE 9</ie_conditional>
				</ie9>
				<ie8>
					<href></href>
					<is_internal>1</is_internal>
					<ie_conditional>IE 8</ie_conditional>
				</ie8>
				<ie7>
					<href></href>
					<is_internal>1</is_internal>
					<ie_conditional>lte IE 7</ie_conditional>
				</ie7>
			</head_css>
			
			<links>
				<favicon>
					<rel>shortcut icon</rel>
					<href>favicon.ico</href>
					<is_image>1</is_image>
				</favicon>
				<iphone>
					<rel>apple-touch-icon-precomposed</rel>
					<href>apple-touch-iphone.png</href>
					<is_image>1</is_image>
				</iphone>
				<ipad>
					<rel>apple-touch-icon-precomposed</rel>
					<href>apple-touch-ipad.png</href>
					<is_image>1</is_image>
					<attributes>
						<sizes>72x72</sizes>
					</attributes>
				</ipad>
				<iphoneretina>
					<rel>apple-touch-icon-precomposed</rel>
					<href>apple-touch-iphone-retina.png</href>
					<is_image>1</is_image>
					<attributes>
						<sizes>114x114</sizes>
					</attributes>
				</iphoneretina>
				<ipadretina>
					<rel>apple-touch-icon-precomposed</rel>
					<href>apple-touch-ipad-retina.png</href>
					<is_image>1</is_image>
					<attributes>
						<sizes>144x144</sizes>
					</attributes>
				</ipadretina>
			</links>
			
			<favicon>
				<href></href>
				<is_internal>1</is_internal>
				<ie_conditional></ie_conditional>
			</favicon>
			
			<head_js>
				<application-head-min>
					<src>html5shiv</src>
					<is_internal>1</is_internal>
					<code></code>
					<ie_conditional>lte IE 8</ie_conditional>
				</application-head-min>
			</head_js>
		</head_includes>
	</head>

	<footer>
		<fine_print>
			<copyright>
				<is_anchor>0</is_anchor>
				<symbol>Copyright &#169;</symbol>
				<holder>yourcompany.com</holder>
				<start_date>2013</start_date>
				<show_current_date>1</show_current_date>
			</copyright>
			<name>Your Name</name>
			<rights>All Rights Reserved</rights>
		</fine_print>
		
		<footer_nav>
			<welcome>
				<text>Welcome</text>
				<is_anchor>1</is_anchor>
				<is_internal>1</is_internal>
				<path></path>
				<target>_self</target>
				<title></title>
				<id></id>
				<class></class>				
			</welcome>
		</footer_nav>
		
		<separator>|</separator>
		
		<branding>
			<logo>
				<src></src>
				<alt>Your Logo Here</alt>
				<text></text>
				<is_anchor>1</is_anchor>
				<path></path>
				<target>_self</target>
				<title></title>
				<is_internal>1</is_internal>
				<id></id>
				<class></class>
			</logo>
		</branding>
		
		<footer_js>
			<jquery>
				<src>http://code.jquery.com/jquery-1.8.3.min.js</src>
				<is_internal>0</is_internal>
				<code></code>
				<ie_conditional></ie_conditional>
			</jquery>
			
			<application-footer-min>
				<src>application-footer-min</src>
				<is_internal>1</is_internal>
				<code></code>
				<ie_conditional></ie_conditional>
			</application-footer-min>
		</footer_js>
	</footer>

</root>
