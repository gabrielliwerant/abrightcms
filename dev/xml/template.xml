					<href></href>
					<is_internal>1</is_internal>
					<ie_conditional></ie_conditional>
				</reset>
				<style>
					<href></href>
					<is_internal>1</is_internal>
					<ie_conditional></ie_conditional>
				</style>
				<ie9>
					<href></href>
					<is_internal>1</is_internal>
					<ie_conditional>IE 9</ie_conditional>
				</ie9>
				<ie8>
					<href></href>
					<is_internal>1</is_internal>
					<ie_conditional>IE 8</ie_conditional>
				</ie8>
				<ie7>
					<href></href>
					<is_internal>1</is_internal>
					<ie_conditional>lte IE 7</ie_conditional>
				</ie7>
			</head_css>
			
			<favicon>
				<href></href>
				<is_internal>1</is_internal>
				<ie_conditional></ie_conditional>
			</favicon>
			
			<head_js>
				<paths_development>
					<src>paths_development</src>
					<is_internal>1</is_internal>
					<code></code>
					<ie_conditional></ie_conditional>
				</paths_development>
				<application-head-min>
					<src>html5shiv</src>
					<is_internal>1</is_internal>
					<code></code>
					<ie_conditional>lte IE 8</ie_conditional>
				</application-head-min>
			</head_js>
		</head_includes>
